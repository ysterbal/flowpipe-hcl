{
  "name": "flowpipe-hcl",
  "displayName": "flowpipe-hcl",
  "description": "Flowpipe HCL Language Support",
  "publisher": "vscode-ext-vanzijl-us",
  "repository": "https://github.com/ysterbal/flowpipe-hcl.git",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "package": "vsce package",
    "build": "mkdir -p build && vsce package --out build/"
  },
  "contributes": {
    "languages": [
      {
        "id": "flowpipe",
        "aliases": [
          "flowpipe-hcl",
          "flowpipe"
        ],
        "extensions": [
          ".fp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "flowpipe",
        "scopeName": "source.flowpipe",
        "path": "./syntaxes/flowpipe.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}